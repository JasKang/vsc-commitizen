{
  "name": "vsc-commitizen",
  "displayName": "vsc-commitizen",
  "description": "vscode commitizen support",
  "version": "0.6.0",
  "publisher": "jaskang",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vsc-commitizen.commit"
  ],
  "main": "./out/extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/JasKang/vsc-commitizen.git"
  },
  "author": {
    "name": "jaskang",
    "email": "jaskang@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasKang/vsc-commitizen/issues"
  },
  "homepage": "https://github.com/JasKang/vsc-commitizen",
  "contributes": {
    "commands": [
      {
        "command": "vsc-commitizen.commit",
        "title": "commit",
        "category": "vsc-commitizen"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^5.0.4",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.1.4",
    "vscode": "^1.1.25"
  },
  "dependencies": {
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1"
  }
}
